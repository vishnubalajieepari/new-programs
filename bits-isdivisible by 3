	int isDivisible(string s)
	{
	    int e=0,o=0; 
	    for(int i=0;i<s.size();i++)
	    {
	        if(s[i]=='1')
	        {
	           if(i%2)
	           {
	               o++;
	           }
	           else
	           {
	               e++;
	           }
	        }
	    }
	    int num=max(o-e,e-o);
	   return num%3==0;
	}
